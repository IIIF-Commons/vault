{
  "name": "@iiif/vault",
  "version": "0.9.19",
  "license": "MIT",
  "main": "./dist/bundle/cjs/index.js",
  "module": "./dist/bundle/esm/index.mjs",
  "types": "dist/index.d.ts",
  "type": "commonjs",
  "files": [
    "dist",
    "npm"
  ],
  "exports": {
    ".": {
      "node": {
        "require": "./dist/node/cjs/index.js",
        "import": "./dist/node/esm/index.mjs",
        "default": "./dist/node/cjs/index.js"
      },
      "browser": {
        "require": "./dist/bundle/cjs/index.js",
        "import": "./dist/bundle/esm/index.mjs"
      }
    },
    "./global-vault": {
      "require": "./npm/global-vault.js",
      "import": "./npm/global-vault.mjs"
    },
    "./actions": {
      "require": "./dist/actions/cjs/index.js",
      "import": "./dist/actions/esm/index.mjs"
    },
    "./store": {
      "require": "./dist/store/cjs/index.js",
      "import": "./dist/store/esm/index.mjs"
    },
    "./utility": {
      "require": "./dist/utility/cjs/index.js",
      "import": "./dist/utility/esm/index.mjs"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/index.d.ts"
      ],
      "actions": [
        "dist/actions/index.d.ts"
      ],
      "store": [
        "dist/store/index.d.ts"
      ],
      "utility": [
        "dist/utility/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "node scripts/build.mjs",
    "prepublishOnly": "node ./scripts/build.mjs && node scripts/validate.mjs"
  },
  "dependencies": {
    "@iiif/parser": "https://pkg.csb.dev/IIIF-Commons/parser/commit/3ce41659/@iiif/parser",
    "@iiif/presentation-2": "1.*",
    "@iiif/presentation-3": "2.*",
    "typesafe-actions": "^5.1.0",
    "zustand": "^4.0.0"
  },
  "optionalDependencies": {
    "node-fetch": "^2.6.7 || ^3.2.10"
  },
  "devDependencies": {
    "@iiif/presentation-3-normalized": "^0.9.7",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "chalk": "^5.0.1",
    "dts-bundle-generator": "^6.12.0",
    "eslint": "^8.21.0",
    "eslint-plugin-prettier": "^4.2.1",
    "execa": "^6.1.0",
    "happy-dom": "^6.0.4",
    "mitt": "^3.0.0",
    "prettier": "^2.7.1",
    "terser": "^5.14.2",
    "tiny-invariant": "^1.2.0",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.4",
    "vitest": "^0.20.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
